{
  "extends": "airbnb",
  "globals": {
    "document": true
  },
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "object-curly-newline": [
      "error",
      {
        "ObjectExpression": "always",
        "ObjectPattern": {
          "multiline": true
        }
      }
    ]
  }
}
